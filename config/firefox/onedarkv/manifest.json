      "toolbar_field": "#1e2227",
      "toolbar_field_focus": "#1e2227",
      "toolbar_field_text": "#abb2bf",
      "toolbar_field_text_focus": "#abb2bf",
      "toolbar_field_border": "transparent",
      "toolbar_field_border_focus": "transparent",
      "toolbar_field_highlight": "#2c313c",
      "toolbar_field_highlight_text": "#abb2bf",
      "toolbar_bottom_separator": "transparent",
      "toolbar_vertical_separator": "transparent"
